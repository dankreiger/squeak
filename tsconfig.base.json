        "libs/frontend/shared/types/src/index.ts"
      ],
      "@squeak-frontend/shared.ui": ["libs/frontend/shared/ui/src/index.ts"],
      "@squeak-frontend/shared.utils": [
        "libs/frontend/shared/utils/src/index.ts"
      ],
      "@squeak-frontend/web.i18n": ["libs/frontend/web/i18n/src/index.ts"],
      "@squeak-frontend/web.ui": ["libs/frontend/web/ui/src/index.ts"],
      "@squeak-shared.constants": ["libs/shared/constants/src/index.ts"],
      "@squeak-shared.enums": ["libs/shared/enums/src/index.ts"],
      "@squeak-shared.fixtures": ["libs/shared/fixtures/src/index.ts"],
      "@squeak-shared.types": ["libs/shared/types/src/index.ts"],
      "@squeak-shared.utils": ["libs/shared/utils/src/index.ts"]
